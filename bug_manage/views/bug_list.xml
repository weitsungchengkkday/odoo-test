<odoo>
    <data>

        <!-- bug list view -->
        <record model="ir.ui.view" id="bug_manage.bug_list">
            <field name="name">Bug 列表</field>
            <field name="model">bm.bug</field>
            <field name="arch" type="xml">
              <tree>
                <field name="name" />
                <field name="is_closed" />
                <field name="user_id" />
                <field name="id" />
              </tree>
            </field>
        </record>

    </data>
</odoo>
